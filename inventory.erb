[OSEv3:children]
masters
nodes
nfs

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v3.6.0
openshift_image_tag=v3.6.0
openshift_public_hostname=apps.teammachine.us
openshift_master_default_subdomain=apps.teammachine.us
openshift_hosted_metrics_deploy=true
enable_excluders=false
openshift_disable_check=docker_storage

[etcd]
apps.teammachine.us

[nfs]
apps.teammachine.us

[masters]
apps.teammachine.us openshift_schedulable=true

[nodes]
apps.teammachine.us openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
